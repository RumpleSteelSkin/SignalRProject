@model CreateFeatureDto

@{
	ViewBag.Title = "title";
	Layout = "~/Views/AdminLayout/Index.cshtml";
}

<div class="content">
	<div class="container-fluid">
		<h4 class="page-title">Feature Processes</h4>
		<div class="row">
			<div class="col-md-12">
				<form method="post">
					<div class="card">
						<div class="card-header">
							<div class="card-title">New Feature</div>
						</div>
						<div class="card-body">
							<div class="form-group">
								<label for="Title">Title</label>
								<input type="text" class="form-control" id="Title" asp-for="Title"
								       placeholder="Enter Feature Title">
							</div>

							<div class="form-group">
								<label for="Description">Description</label>
								<input type="text" class="form-control" id="Description" asp-for="Description"
								       placeholder="Enter Feature Price">
							</div>
							
							<div class="form-group">
								<button class="btn btn-primary">Feature Add</button>
							</div>
						</div>
					</div>
				</form>
			</div>
		</div>
	</div>
</div>
