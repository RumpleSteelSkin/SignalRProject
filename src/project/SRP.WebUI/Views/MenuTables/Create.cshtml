@model CreateMenuTableDto

@{
	ViewBag.Title = "title";
	Layout = "~/Views/AdminLayout/Index.cshtml";
}

<div class="content">
	<div class="container-fluid">
		<h4 class="page-title">MenuTable Processes</h4>
		<div class="row">
			<div class="col-md-12">
				<form method="post">
					<div class="card">
						<div class="card-header">
							<div class="card-title">New MenuTable</div>
						</div>
						<div class="card-body">
							<div class="form-group">
								<label for="Name">Name</label>
								<input type="text" class="form-control" id="Name" asp-for="Name"
								       placeholder="Enter MenuTable Name">
							</div>
							
							<div class="form-group">
								<button class="btn btn-primary">MenuTable Add</button>
							</div>
						</div>
					</div>
				</form>
			</div>
		</div>
	</div>
</div>
