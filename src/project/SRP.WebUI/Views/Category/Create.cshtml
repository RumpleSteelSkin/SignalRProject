@model CreateCategoryDto

@{
	ViewBag.Title = "title";
	Layout = "~/Views/AdminLayout/Index.cshtml";
}

<div class="content">
	<div class="container-fluid">
		<h4 class="page-title">Category Processes</h4>
		<div class="row">
			<div class="col-md-12">
				<form method="post">
					<div class="card">
						<div class="card-header">
							<div class="card-title">New Category</div>
						</div>
						<div class="card-body">
							<div class="form-group">
								<label for="CategoryName">Enter Category Name</label>
								<input type="text" class="form-control" id="CategoryName" asp-for="Name" placeholder="Enter Category Name">
							</div>
							<div class="form-group">
								<button class="btn btn-primary">Category Add</button>
							</div>
						</div>
					</div>
				</form>
			</div>
		</div>
	</div>
</div>
